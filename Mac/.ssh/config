# Gitlab CERN
Host gitlab.web.cern.ch
	HostName gitlab.web.cern.ch
	User <CERN-USERNAME>
	IdentityFile ~/.ssh/id_rsa

# Github smyrnakis
Host github.com
	HostName github.com
	User <PERSONAL-EMAIL>
	IdentityFile ~/.ssh/id_rsa_git

# lxplus as proxy
# https://security.web.cern.ch/security/recommendations/en/ssh_browsing.shtml
Host lxplus*.cern.ch
   DynamicForward 12345
